{
  "algorithms": ["FilterBoost", "AdaBoost"],

  "estimator": {
    "estimator_type": "stump",
    "estimator_params": { "max_depth": [1] }
  },

  "test": {
    "test_size": 0.15,
    "validation_size" : 0.15,
    "random_state": 42,
    "N_synthetic_tests": 0,
    "multiprocessing" : false,
    "use_predefined_datasets": true
  },

  "model": {
    "common": {
      "n_estimators": [ 1024 ],
      "learning_rate":  [ 0.001, 1]
    },
    "FilterBoost": {
      "epsilon" : [0.1],
      "delta" : [0.9],
      "tau" : [0.1]
    }
  }
}
