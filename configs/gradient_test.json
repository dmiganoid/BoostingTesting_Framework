{
  "algorithms": ["CatBoost", "XGBoost", "LightGBM", "NGBoost", "SMOTEBoost", "RUSBoost", "GradientBoost", "AdaBoost", "SOWAdaBoost"],

  "estimator": {
    "estimator_type": "stump",
    "estimator_params": { "max_depth": [1] }
  },

  "test": {
    "test_size": 0.15,
    "validation_size": 0.15,
    "artificial_noise" : 0,
    "retrain" : 11,
    "random_state": 42,
    "N_synthetic_tests": 0,
    "multiprocessing" : 8,
    "use_predefined_datasets": true,
    "predefined_datasets" : [
        "wisconsin_breast_cancer_prepared.csv",
        "banana_quality.csv",
        "keel_vehicle3_prepared.csv", 
        "dataset-generated-2.csv",
        "dataset-generated-3.csv",
        "dataset-generated-4.csv",
        "dataset-generated-5.csv",
        "dataset-generated-6.csv",
        "dataset-generated-8.csv",
        "dataset-generated-9.csv",
        "dataset-generated-12.csv"
      ]
    },

  "model": {
    "common": {
    "n_estimators": [ 2, 4, 8, 16, 32, 64, 128, 256, 512, 1024, 2048],
    "learning_rate":  [ 0.001, 0.032, 0.128, 0.256, 0.512, 1]
    },
    "CatBoost": {
      "depth": [1]
    },
    "XGBoost": {
      "max_depth": [1]
    },
    "LightGBM": {
      "max_depth": [1]
    },
    "GradientBoost": {
      "gpu": false,
      "loss": ["log_loss"]
    },
    "SOWAdaBoost": {
      "gpu": false
    }
  }
}
  