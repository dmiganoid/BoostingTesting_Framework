{
  "algorithms": ["CatBoost", "XGBoost", "LightGBM", "NGBoost", "SMOTEBoost", "RUSBoost", "GradientBoost", "AdaBoost", "SOWAdaBoost"],

  "estimator": {
    "estimator_type": "stump",
    "estimator_params": { "max_depth": [1] }
  },

  "test": {
  "test_size": 0.15,
  "validation_size":0.15,
  "artificial_noise" : 0,
  "retrain" : 6,
  "random_state": 42,
  "N_synthetic_tests": 1,
  "multiprocessing" : 8,
  "use_predefined_datasets": true,
  "skip_datasets" : [
    "creditcard.csv",
    "abalone_prepared.csv",
    "smart_grid_stability_augmented.csv"
  ]
  },

  "model": {
    "common": {
    "n_estimators": [ 2, 4, 8, 16, 32, 64, 128, 256, 512, 1024, 2048],
    "learning_rate":  [ 0.001, 0.032, 0.128, 0.256, 0.512, 1]
    },
    "CatBoost": {
      "depth": [1]
    },
    "XGBoost": {
      "max_depth": [1]
    },
    "LightGBM": {
      "max_depth": [1]
    },
    "GradientBoost": {
      "gpu": false,
      "loss": ["log_loss"]
    },
    "SOWAdaBoost": {
      "gpu": false
    }
  }
}
  